  "scripts": {
    "test":       "NODE_ENV=test mocha test/init-node.js 'test/**/*spec.js' 'lib/**/*spec.js' --reporter spec",
    "test-watch": "NODE_ENV=test mocha test/init-node.js 'test/**/*spec.js' 'lib/**/*spec.js' --reporter min --watch",
    "test-debug": "echo '**** TO SEE TESTS RUN `node-inspector` ****' && NODE_ENV=test mocha test/init-node.js 'test/**/*spec.js' 'lib/**/*spec.js' --reporter spec --debug --debug-brk",
    "test-browser": "NODE_ENV=test gulp test --debug --port=9001",
    "build":      "NODE_ENV=production gulp build",
    "start":      "NODE_ENV=development gulp start --debug"
  },
